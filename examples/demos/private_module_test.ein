# private_module_test.ein - Test module with both public and private functions

# Public function - can be imported
pub fn public_function(x) {
    x * 2
}

# Private function - should NOT be importable
fn private_function(x) {
    x * 3
}

# Test the functions locally
print("=== Private Module Test ===");
print("Local public_function(5):", public_function(5));
print("Local private_function(5):", private_function(5));
print("Both functions work locally âœ“");
